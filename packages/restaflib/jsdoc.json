{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
      "include": ["src"],      
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "../../jsdocs/restaflib",
      "recurse": true,
      "readme": "README.md"
    }
    }
    