{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
      "include": ["./a.jsdoc"],
      "includePattern": ".js",
      "excludePattern": "(node_modules)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./jsdocs",
      "recurse": false,
      "readme": "./README.md"
    }
}
    