{
  "name": "restaf-uidemos",
  "version": "3.4.0",
  "description": "UI demos for Viya using restaf, restaflib and uilibrary",
  "main": "index.js",
  "scripts": {
    "implicit": "npx @sassoftware/viya-appserverjs --appenv=./appenv.js --env=./overrideImplicit.env --docker=./Dockerfile",
    "code": "npx @sassoftware/viya-appserverjs --appenv=./appenv.js --env=./.env --docker=./Dockerfile",
    "rproxy": "npx @sassoftware/viya-appserverjs  --appenv=./appenv.js --env=./overrideCoder.env --docker=./Dockerfile",
    "indocker": "npx @sassoftware/viya-appserverjs",
    "buildapp": "docker build -f Dockerfile -t viyaapp .",
    "runapp": "docker run -p 8080:8080  --env-file overrides.env  -t viyaapp"
  },
  "repository": "https://github.com/sassoftware/restaf/tree/main/packages/uidemos",
