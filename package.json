{
  "name": "restaf",
  "version": "3.0.7",
  "description": "Library for writing SAS Viya application using REST APIs",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "keywords": [
    "restaf",
    "SAS",
    "Viya",
    "REST API"
  ],
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.4",
    "yarn": ">=1.22.0"
  },
  "main": "lib/restaf",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": "https://github.com/sassoftware/restaf.git",
  "bugs": {
    "url": "https://github.com/sassoftware/restaf/issues"
  },
  "scripts": {
    "test": "cd ./packages/examples && npm test",
    "examples:debug": "cd ./packages/examples && npm run debug",
    "examples:dkrbuild": "cd ./packages/examples && npm run dkrbuild",
    "examples:dkrrun": "cd ./packages/examples && npm run dkrrun",
    "examples:dkrup": "cd ./packages/examples && docker-compose up",
    "restaf:build": "cd ./packages/restaf && yarn run build && npx cp -r ./dist/restaf.js ../uidemos/public/shared/dist/restaf.js",
    "restaf:pub": "cd ./packages/restaf && npm run pub",
    "restaflib:build": "cd ./packages/restaflib && yarn run build  && npx cp -r ./dist/restaflib.js ../uidemos/public/shared/dist/restaflib.js",
    "restaflib:pub": "cd ./packages/restaflib && npm run pub",
    "restafuilib:build": "cd ./packages/restafuilib && yarn run build  && npx cp -r ./dist/restafuilib.js ../uidemos/public/shared/dist/restafuilib.js",
    "uidemos:implicit": "cd ./packages/uidemos && npm run implicit",
    "uidemos:code": "cd ./packages/uidemos && npm run code",
    "uidemos:rproxy": "cd ./packages/uidemos && npm run rproxy",
    "registerclient": "cd ./packages/registerclient && npm start",
    "registerclient:pub": "cd ./packages/registerclient && npm run pub",
    "viyacaddy": "cd ./packages/viyacaddy && npm start",
    "viyacaddy:pub": "cd ./packages/viyacaddy && npm run pub",
    "buildall": "npm run restaf:build && npm run restaflib:build && npm run restafuilib:build",
    "puball": "yarn restaf:pub && yarn restaflib:pub && yarn viyacaddy:pub && yarn registerclient:pub",
    "token": "node token.js",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.js'",
    "lint": "eslint . --ext .js --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/plugin-transform-spread": "^7.12.1",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.3",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "prettier": "^2.2.1",
    "prettyjson": "^1.2.1",
    "rimraf": "^3.0.2",
    "set-versions": "^1.0.3",
    "showdown": "^1.9.1",
    "shx": "^0.3.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "win-ca": "^3.4.5",
    "yargs": "^16.2.0"
  },
  "resolutions": {
    "**/lodash": ">=4.17.15",
    "**/yargs-parser": ">=13.1.2",
    "**/node-notifier": "^8.0.1"
  },
  "dependencies": {
    "@sassoftware/restaf-server": "^8.2.12",
    "aws-sdk": "^2.832.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "jwt-decode": "^3.1.2",
    "lodash": ">=4.17.20",
    "react-json-pretty": "^2.2.0",
    "serverless": "^2.21.1"
  }
}
