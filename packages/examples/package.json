  "license": "Apache-2.0",
  "keywords": [
    "restaf",
    "SAS",
    "Viya",
    "REST API"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=22.16.0",
    "npm": ">=10.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sassoftware/restaf/tree/master/packages/examples"
  },
  "bugs": {
    "url": "https://github.com/sassoftware/restaf/issues"
  },
  "scripts": {
    "start": "node",
    "prepublishOnly": "npm run build",
    "test": "cross-env RESTAFENV=.env NODE_TLS_REJECT_UNAUTHORIZED=0  npx jest",
    "debug": "cross-env RESTAFENV=.env NODE_TLS_REJECT_UNAUTHORIZED=0 node --inspect-brk ../../node_modules/jest/bin/jest.js --runInBand",
    "lint": "eslint . --fix",
    "dkrbuild": "docker build -f Dockerfile -t ruthere .",
    "dkrrun": "docker run --env-file ./override.env  -t ruthere"
  },
  "jest": {
    "testTimeout": 10000000,
    "verbose": false
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "microbundle": "^0.15.1"
  }
}
